    "ioredis": "^5.3.2",
    "pg": "^8.11.3",
    "pino-pretty": "^13.1.1",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/pg": "^8.10.7",
    "@types/uuid": "^9.0.6",
    "bun-types": "latest"
