{
  "name": "order-execution-engine",
  "version": "1.0.0",
  "description": "Order execution engine with DEX routing and WebSocket status updates",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "start": "bun run src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target bun",
    "init-db": "bun run init-db.ts",
    "init-db:js": "node init-db.js",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "test:integration": "bun test test/integration.test.ts",
    "test:api": "bun test test/api.test.ts",
    "test:websocket": "bun test test/websocket.test.ts",
    "test:websocket-integration": "bun test test/websocket-integration.test.ts",
    "test:load": "bun test test/load.test.ts",
    "test:all": "bun run test/run-tests.ts",
    "test:ws-manual": "node test-websocket-manual.js"
  },
  "keywords": ["order-execution", "dex", "websocket", "trading"],
  "author": "Order Execution Engine",
  "license": "MIT",
  "dependencies": {
    "@fastify/websocket": "^8.3.1",
    "bullmq": "^4.15.4",
    "dotenv": "^16.3.1",
    "fastify": "^4.24.3",
    "ioredis": "^5.3.2",
    "pg": "^8.11.3",
    "pino-pretty": "^13.1.1",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/pg": "^8.10.7",
    "@types/uuid": "^9.0.6",
    "@types/ws": "^8.18.1",
    "bun-types": "latest",
    "tsx": "^4.6.2"
  }
}